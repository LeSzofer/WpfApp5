<Window x:Class="WpfApp5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WPF App" Height="400" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal">
            <Button Content="Page 1" Click="ButtonPage1_Click" Height="200" Width="200"/>
            <Button Content="Page 2" Click="ButtonPage2_Click" Height="200" Width="200"/>
            <Button Content="Page 4" Click="ButtonPage4_Click" Height="200" Width="200"/>
            <Button Content="Page 3" Click="ButtonPage3_Click" Height="200" Width="200"/>
        </StackPanel>

        <Frame x:Name="MainFrame" Grid.Row="1"/>
    </Grid>
</Window>
